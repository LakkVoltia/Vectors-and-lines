#include <iostream>
#include <string>

bool Nice (const std::string& password) {
    if (password.size() < 8 || password.size() > 14) {
        return false;
    }
    int Big = 0;
    int Short = 0;
    int Number = 0;
    int Special = 0;

    for (char c : password) {
        if (c < 33 || c > 126) {
            return false;
        }
        if ('A' <= c && c <= 'Z') {
            Big = 1;
        } else if ('a' <= c && c <= 'z') {
            Short = 1;
        } else if ('0' <= c && c <= '9') {
            Number = 1;
        } else {
            Special = 1;
        }
    }

    return Big + Short + Number + Special >= 3;
}

int main() {
    std::string password;
    std::getline(std::cin, password);
    if ( Nice (password)) {
        std::cout << "YES\n";
    } else {
        std::cout << "NO\n";
    }
}
