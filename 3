#include <iostream>
#include <vector>
 using namespace std;
int main()
{
    int n;
    cin>> n;
    vector<int> seats(n);
    for(int guest = 1; guest<= n ; ++guest){
        int seat;
        cin>> seat;
        seats [seat - 1] = guest;
    }
    for(int guest :seats){
        cout<< guest << " ";
    }
        cout << "\n";
}
